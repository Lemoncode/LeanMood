{
  "name": "lm-app",
  "version": "1.0.0",
  "description": "LeanMood web application",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build:prod",
    "start": "nf start",
    "start:dev": "webpack-dev-server --config ./config/webpack/app/webpack.config.dev.js",
    "start:prod": "node server",
    "start:rest": "cross-env REST_ENV=real webpack-dev-server --config ./config/webpack/app/webpack.config.dev.js",
    "build": "npm run build:dev",
    "build:dev": "rimraf dist && webpack --config ./config/webpack/app/webpack.config.dev.js",
    "build:prod": "rimraf public && cross-env NODE_ENV=production webpack -p --config ./config/webpack/app/webpack.config.prod.js",
    "lint": "tslint -c ./tslint.json --force src/**/*.ts src/**/*.tsx",
    "test": "karma start ./config/karma/karma.config.test.js",
    "test:watch": "karma start ./config/karma/karma.config.test.js --no-single-run",
    "test:coverage": "node --max_old_space_size=4096 node_modules/karma/bin/karma start ./config/karma/karma.config.coverage.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MasterLemon2016/LeanMood.git"
  },
  "keywords": [
    "LeanMood",
    "Online",
    "Training"
  ],
