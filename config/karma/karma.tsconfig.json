{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": false,
    "noImplicitAny": false,
    "sourceMap": false,
    "inlineSourceMap": true,
    "jsx": "react",
    "noLib": false,
    "suppressImplicitAnyIndexErrors": true,
    "types": [
      "karma-chai-sinon",
      "webpack-env",
      "node"
    ],
    "typeRoots": [
      "../../node_modules/@types"
    ]
  },
  "compileOnSave": false,
  "exclude": [
    "node_modules"
  ]
}
